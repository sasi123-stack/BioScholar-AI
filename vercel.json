{
    "version": 2,
    "builds": [
        {
            "src": "src/api/app.py",
            "use": "@vercel/python"
        },
        {
            "src": "frontend/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/src/api/app.py"
        },
        {
            "src": "/(.*)",
            "dest": "/frontend/$1"
        }
    ],
    "env": {
        "LOW_MEMORY_MODE": "true"
    }
}